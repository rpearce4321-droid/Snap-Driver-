{
  "name": "snapdriver",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n WEB,API -c blue,green \"vite\" \"npm:server\"",
    "build": "tsc -b && vite build",
    "test": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "nodemon --watch server --ext js --signal SIGINT server/index.js",
    "db:seed": "node server/db/seed.js",
    "seed:full": "node prisma/seeders/seed-full.mjs"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@prisma/client": "^6.18.0",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jspdf": "^4.1.0",
    "keen-slider": "^6.8.6",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.30.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "wrangler": "^4.61.0"
  }
